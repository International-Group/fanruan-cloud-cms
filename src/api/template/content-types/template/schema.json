{
  "kind": "collectionType",
  "collectionName": "templates",
  "info": {
    "singularName": "template",
    "pluralName": "templates",
    "displayName": "Template",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "template_status": {
      "type": "enumeration",
      "required": true,
      "default": "public",
      "enum": [
        "public",
        "private",
        "delisted",
        "pending"
      ]
    },
    "template_link": {
      "type": "text",
      "required": true
    },
    "author": {
      "type": "string",
      "default": "FanRuan",
      "required": true
    },
    "tags": {
      "type": "component",
      "component": "shared.tag",
      "repeatable": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "scenario": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Financial Performance",
        "Revenue Optimization",
        "Operational Excellence",
        "Customer Experience",
        "Supply Chain Intelligence",
        "Workforce Analytics",
        "Quality Assurance",
        "Strategic Planning"
      ]
    },
    "industry": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Manufacturing",
        "Supply Chain",
        "Retail",
        "E-commerce",
        "Public Sector",
        "Real Estate",
        "Healthcare",
        "Universal Solutions",
        "Logistics"
      ]
    },
    "language": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "en-us",
        "zh-tw"
      ]
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false,
      "required": true
    },
    "free": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "price": {
      "type": "integer",
      "required": false
    },
    "featured": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "product": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "FineBI",
        "FineReport",
        "FineVis",
        "FineDataLink"
      ]
    },
    "description": {
      "type": "richtext",
      "required": false
    },
    "download_link": {
      "type": "string",
      "default": "#",
      "required": true
    },
    "supported_version": {
      "type": "string",
      "required": true
    },
    "saved": {
      "type": "integer"
    },
    "download_file": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    }
  }
}

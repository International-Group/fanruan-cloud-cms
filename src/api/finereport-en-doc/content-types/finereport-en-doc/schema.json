{
  "kind": "collectionType",
  "collectionName": "finereport_en_docs",
  "info": {
    "singularName": "finereport-en-doc",
    "pluralName": "finereport-en-docs",
    "displayName": "finereport-en-doc"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "summary": {
      "type": "string",
      "required": false
    },
    "content": {
      "type": "richtext",
      "required": false
    },
    "dir": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "doc_children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::finereport-en-doc.finereport-en-doc",
      "mappedBy": "doc_parent"
    },
    "doc_parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::finereport-en-doc.finereport-en-doc",
      "inversedBy": "doc_children"
    },
    "display_order": {
      "type": "integer",
      "default": 999
    }
  }
}
